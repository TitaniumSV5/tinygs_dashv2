{
    "version": 2,
    "builds": [
      {
        "src": "backend/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "frontend/dist" }
      }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/backend" }, { "source": "/(.*)", "destination": "/frontend/dist/$1" }]
    
  }
  