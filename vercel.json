{
    "version": 2,
    "builds": [
      {
        "src": "backend/package.json",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [{ "source": "/api/(.*)", "destination": "/backend" }],
    "crons": [
      {
        "path": "/fetch-packets",
        "schedule": "0 6 * * *"
      }
    ]
  }
  