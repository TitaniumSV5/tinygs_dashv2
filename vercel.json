{
    "version": 2,
    "builds": [
      {
        "src": "backend/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "backend/fetchAndStorePackets.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/backend" }],
    "crons": [
      {
        "path": "/api/fetchAndStorePackets",
        "schedule": "*/5 * * * *"
      }
    ]
  }
  